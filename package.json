{
  "name": "dynamic-nft-sample",
  "private": true,
  "scripts": {
    "dev": "NETWORK=rinkeby next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "script": "npx hardhat run scripts/sample-script.js",
    "deploy-rinkeby": "NETWORK=rinkeby npx hardhat run scripts/sample-script.js --network rinkeby",
    "deploy-mainnet": "NETWORK=matic npx hardhat run scripts/deploy.js --network mainnet",
    "c": "NETWORK=local npx hardhat console",
    "export": "NETWORK=matic next build && NETWORK=matic next export",
    "deploy": "firebase deploy"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.4.2",
    "next": "12.0.8",
    "react": "17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "17.0.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.1.1"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "chai": "^4.2.0",
    "dotenv": "^14.3.0",
    "eslint": "8.7.0",
    "eslint-config-next": "12.0.8",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.5.3",
    "hardhat": "^2.8.3"
  }
}
